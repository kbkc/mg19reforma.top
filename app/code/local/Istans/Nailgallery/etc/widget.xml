<widgets>
    <nailgallery_gallery_view type="istans_nailgallery/gallery_widget_view" translate="name description" module="istans_nailgallery">
        <name>Gallery view</name>
        <description>Gallery view widget</description>
        <is_email_compatible>0</is_email_compatible>
        <parameters>
            <gallery_id type="complex" translate="label">
                <visible>1</visible>
                <required>1</required>
                <label>Gallery</label>
                <type>label</type>
                <helper_block>
                    <type>istans_nailgallery/adminhtml_gallery_widget_chooser</type>
                    <data>
                        <button translate="open">
                            <open>Select Gallery</open>
                        </button>
                    </data>
                </helper_block>
                <sort_order>120</sort_order>
            </gallery_id>
        </parameters>
